{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "types": ["kakao.maps.d.ts"]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/components/units/Boards/write/BoardWrite.container.ts"],
  "exclude": ["node_modules"]
}
